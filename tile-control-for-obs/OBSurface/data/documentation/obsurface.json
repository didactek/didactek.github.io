vigationElement"}],"abstract":[],"identifier":"doc:\/\/OBSurface\/documentation\/OBSurface\/NavigationElement","kind":"symbol","type":"topic","navigatorTitle":[{"kind":"identifier","text":"NavigationElement"}],"url":"\/documentation\/obsurface\/navigationelement"},"doc://OBSurface/documentation/OBSurface/FitPolicyView":{"role":"symbol","title":"FitPolicyView","fragments":[{"kind":"keyword","text":"struct"},{"kind":"text","text":" "},{"kind":"identifier","text":"FitPolicyView"}],"abstract":[],"identifier":"doc:\/\/OBSurface\/documentation\/OBSurface\/FitPolicyView","kind":"symbol","type":"topic","navigatorTitle":[{"kind":"identifier","text":"FitPolicyView"}],"url":"\/documentation\/obsurface\/fitpolicyview"},"doc://OBSurface/documentation/OBSurface/SourceMorpher":{"role":"symbol","title":"SourceMorpher","fragments":[{"kind":"keyword","text":"struct"},{"kind":"text","text":" "},{"kind":"identifier","text":"SourceMorpher"}],"abstract":[{"type":"text","text":"Policy\/procedure for setting up source transformations."}],"identifier":"doc:\/\/OBSurface\/documentation\/OBSurface\/SourceMorpher","kind":"symbol","type":"topic","navigatorTitle":[{"kind":"identifier","text":"SourceMorpher"}],"url":"\/documentation\/obsurface\/sourcemorpher"},"doc://OBSurface/documentation/OBSurface/RecordControlView":{"role":"symbol","title":"RecordControlView","fragments":[{"kind":"keyword","text":"struct"},{"kind":"text","text":" "},{"kind":"identifier","text":"RecordControlView"}],"abstract":[],"identifier":"doc:\/\/OBSurface\/documentation\/OBSurface\/RecordControlView","kind":"symbol","type":"topic","navigatorTitle":[{"kind":"identifier","text":"RecordControlView"}],"url":"\/documentation\/obsurface\/recordcontrolview"},"doc://OBSurface/documentation/OBSurface/SettingsView":{"role":"symbol","title":"SettingsView","fragments":[{"kind":"keyword","text":"struct"},{"kind":"text","text":" "},{"kind":"identifier","text":"SettingsView"}],"abstract":[],"identifier":"doc:\/\/OBSurface\/documentation\/OBSurface\/SettingsView","kind":"symbol","type":"topic","navigatorTitle":[{"kind":"identifier","text":"SettingsView"}],"url":"\/documentation\/obsurface\/settingsview"},"doc://OBSurface/documentation/OBSurface/CodableRaw":{"role":"symbol","title":"CodableRaw","fragments":[{"kind":"keyword","text":"struct"},{"kind":"text","text":" "},{"kind":"identifier","text":"CodableRaw"}],"abstract":[{"type":"text","text":"Wrapper that leverages a struct’s Codable conformance to make it RawRepresentable."}],"identifier":"doc:\/\/OBSurface\/documentation\/OBSurface\/CodableRaw","kind":"symbol","type":"topic","navigatorTitle":[{"kind":"identifier","text":"CodableRaw"}],"url":"\/documentation\/obsurface\/codableraw"}}}
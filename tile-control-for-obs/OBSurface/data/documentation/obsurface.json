ic","navigatorTitle":[{"kind":"identifier","text":"RationalLogScale"}],"url":"\/documentation\/obsurface\/rationallogscale"},"doc://OBSurface/documentation/OBSurface/FrameTrimSetting_Previews":{"role":"symbol","title":"FrameTrimSetting_Previews","fragments":[{"kind":"keyword","text":"struct"},{"kind":"text","text":" "},{"kind":"identifier","text":"FrameTrimSetting_Previews"}],"abstract":[],"identifier":"doc:\/\/OBSurface\/documentation\/OBSurface\/FrameTrimSetting_Previews","kind":"symbol","type":"topic","navigatorTitle":[{"kind":"identifier","text":"FrameTrimSetting_Previews"}],"url":"\/documentation\/obsurface\/frametrimsetting_previews"},"doc://OBSurface/documentation/OBSurface":{"role":"collection","title":"OBSurface","abstract":[{"type":"text","text":"A SwiftUI application that dynamically arranges\/tiles\/crops OSB sources inside a scene."},{"type":"text","text":" "},{"type":"text","text":"Operates OBS from a MIDI control surface."}],"identifier":"doc:\/\/OBSurface\/documentation\/OBSurface","kind":"symbol","type":"topic","url":"\/documentation\/obsurface"},"doc://OBSurface/documentation/OBSurface/logger":{"role":"symbol","title":"logger","fragments":[{"kind":"keyword","text":"let"},{"kind":"text","text":" "},{"kind":"identifier","text":"logger"},{"kind":"text","text":": "},{"kind":"typeIdentifier","text":"Logger","preciseIdentifier":"s:7Logging6LoggerV"}],"abstract":[],"identifier":"doc:\/\/OBSurface\/documentation\/OBSurface\/logger","kind":"symbol","type":"topic","url":"\/documentation\/obsurface\/logger"},"doc://OBSurface/documentation/OBSurface/FrameTrimSetting":{"role":"symbol","title":"FrameTrimSetting","fragments":[{"kind":"keyword","text":"struct"},{"kind":"text","text":" "},{"kind":"identifier","text":"FrameTrimSetting"}],"abstract":[],"identifier":"doc:\/\/OBSurface\/documentation\/OBSurface\/FrameTrimSetting","kind":"symbol","type":"topic","navigatorTitle":[{"kind":"identifier","text":"FrameTrimSetting"}],"url":"\/documentation\/obsurface\/frametri